                      </rdf:Description>
                      <owl:Restriction>
                        <owl:someValuesFrom rdf:resource="http://svn.leibnizcenter.org/svn/Estrella/wp1/ontology/lkif-core/expression.owl#Expectation"/>
                        <owl:onProperty>
                          <owl:ObjectProperty rdf:about="#plays"/>
                        </owl:onProperty>
                      </owl:Restriction>
                    </owl:intersectionOf>
                  </owl:Class>
                </owl:someValuesFrom>
              </owl:Restriction>
            </owl:intersectionOf>
          </owl:Class>
        </owl:someValuesFrom>
      </owl:Restriction>
    </owl:equivalentClass>
