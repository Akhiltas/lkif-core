          <owl:ObjectProperty rdf:about="#played_by"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:subClassOf rdf:resource="#Role"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#played_by"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="http://svn.leibnizcenter.org/svn/Estrella/wp1/ontology/lkif-core/lkif-top.owl#Mental_Object"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:ObjectProperty rdf:about="#plays">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="#played_by"/>
    </owl:inverseOf>
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Agents play roles, roles enable certain specific actions</rdfs:comment>
    <rdfs:range rdf:resource="#Role"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#played_by">
    <rdfs:domain rdf:resource="#Role"/>
    <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <owl:inverseOf rdf:resource="#plays"/>
  </owl:ObjectProperty>
</rdf:RDF>

