  <owl:Class rdf:ID="Epistemic_Role">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://svn.leibnizcenter.org/svn/Estrella/wp1/ontology/lkif-core/lkif-top.owl#Mental_Object"/>
