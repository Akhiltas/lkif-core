    <owl:imports rdf:resource="http://www.estrellaproject.org/lkif-core/action.owl"/>
    <owl:imports rdf:resource="http://www.estrellaproject.org/lkif-core/mereology.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="Person_Role">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A role played by some person (i.e. not an organisation)</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="Function"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Social_Role"/>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="played_by"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/action.owl#Person"/>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#played_by"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/action.owl#Person"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Organisation_Role">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
              <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/action.owl#Agent">
                <owl:disjointWith>
                  <owl:Class rdf:ID="Role"/>
                </owl:disjointWith>
                <rdfs:subClassOf>
                  <owl:Restriction>
                    <owl:onProperty>
                      <owl:ObjectProperty rdf:ID="plays"/>
                    </owl:onProperty>
                    <owl:allValuesFrom>
                      <owl:Class rdf:about="#Role"/>
                    </owl:allValuesFrom>
                  </owl:Restriction>
                </rdfs:subClassOf>
              </rdf:Description>
              <owl:Restriction>
                <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/action.owl#Organisation"/>
                <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/mereology.owl#part_of"/>
              </owl:Restriction>
            </owl:intersectionOf>
          </owl:Class>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#played_by"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="#Function"/>
    </owl:disjointWith>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
              <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/action.owl#Agent"/>
              <owl:Restriction>
                <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/action.owl#Organisation"/>
                <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/mereology.owl#part_of"/>
              </owl:Restriction>
            </owl:intersectionOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#played_by"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Social_Role"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A role which has a meaning in the context of an organisation: i.e. the role defines a 'position' within sthe structure of roles within an organisation</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Social_Role">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Role"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A social role is played by some agent in the context of social activities. The social role brings with it some expectation of 'default' behavior of the role-filler.</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="#Function"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#played_by"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/action.owl#Agent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
              <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/action.owl#Agent"/>
              <owl:Restriction>
                <owl:someValuesFrom>
                  <owl:Class>
                    <owl:intersectionOf rdf:parseType="Collection">
                      <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/action.owl#Action">
                        <owl:disjointWith>
                          <owl:Class rdf:about="#Role"/>
                        </owl:disjointWith>
                      </rdf:Description>
                      <owl:Restriction>
                        <owl:onProperty>
                          <owl:ObjectProperty rdf:about="#plays"/>
                        </owl:onProperty>
                        <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#Expectation"/>
                      </owl:Restriction>
                    </owl:intersectionOf>
                  </owl:Class>
                </owl:someValuesFrom>
                <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/action.owl#actor_in"/>
              </owl:Restriction>
            </owl:intersectionOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#played_by"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="Epistemic_Role">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The role of something used in a reasoning/inference process</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/lkif-top.owl#Mental_Object"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#played_by"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Role"/>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#played_by"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/lkif-top.owl#Mental_Object"/>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="#Role">
    <owl:disjointWith rdf:resource="http://www.estrellaproject.org/lkif-core/action.owl#Agent"/>
    <owl:disjointWith rdf:resource="http://www.estrellaproject.org/lkif-core/action.owl#Action"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A role is a specification of default behavior and accompanying expectations of the thing 'playing' the role. Similar to actors in a theater who play roles, but are not the roles. Example: student.</rdfs:comment>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#played_by"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/lkif-top.owl#Mental_Concept"/>
  </owl:Class>
  <owl:Class rdf:about="#Function">
    <owl:disjointWith rdf:resource="#Person_Role"/>
    <owl:disjointWith rdf:resource="#Organisation_Role"/>
    <rdfs:subClassOf rdf:resource="#Role"/>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#played_by"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
              <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/process.owl#Physical_Object"/>
              <owl:Class>
                <owl:complementOf rdf:resource="http://www.estrellaproject.org/lkif-core/action.owl#Agent"/>
              </owl:Class>
            </owl:intersectionOf>
          </owl:Class>
        </owl:someValuesFrom>
      </owl:Restriction>
    </owl:equivalentClass>
    <owl:disjointWith rdf:resource="#Social_Role"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#played_by"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
              <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/process.owl#Physical_Object"/>
              <owl:Class>
                <owl:complementOf rdf:resource="http://www.estrellaproject.org/lkif-core/action.owl#Agent"/>
              </owl:Class>
            </owl:intersectionOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The use or purpose of some object as used in some context.</rdfs:comment>
  </owl:Class>
  <owl:ObjectProperty rdf:about="#plays">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Specifies that some thing plays a role</rdfs:comment>
    <rdfs:range rdf:resource="#Role"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="#played_by"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#played_by">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Specifies that some role is played by some thing</rdfs:comment>
    <owl:inverseOf rdf:resource="#plays"/>
    <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:domain rdf:resource="#Role"/>
  </owl:ObjectProperty>
</rdf:RDF>

