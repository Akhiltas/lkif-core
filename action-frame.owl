<?xml version="1.0"?>
<rdf:RDF
    xmlns:action="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/legal-action.owl#"
    xmlns="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/action-frame.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
  xml:base="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/action-frame.owl">
  <owl:Ontology rdf:about="">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Will describe the framework elements for constructing actions... (action frames)</rdfs:comment>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Created with TopBraid Composer</owl:versionInfo>
    <owl:imports rdf:resource="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/legal-action.owl"/>
  </owl:Ontology>
  <owl:ObjectProperty rdf:ID="patient">
    <owl:equivalentProperty>
      <owl:ObjectProperty rdf:ID="theme"/>
    </owl:equivalentProperty>
    <rdfs:subPropertyOf rdf:resource="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/process.owl#participant"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >patient: the agent or thing the action is performed on </rdfs:comment>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="patient_of"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="recipient_of">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="recipient"/>
    </owl:inverseOf>
    <rdfs:subPropertyOf rdf:resource="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/process.owl#participant_in"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="instrument_in">
    <rdfs:subPropertyOf rdf:resource="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/process.owl#participant_in"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="instrument"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="creates">
    <rdfs:subPropertyOf rdf:resource="#patient"/>
    <rdfs:domain>
      <rdf:Description rdf:about="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/action.owl#Creation">
        <owl:equivalentClass>
          <owl:Restriction>
            <owl:onProperty rdf:resource="#creates"/>
            <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
          </owl:Restriction>
        </owl:equivalentClass>
      </rdf:Description>
    </rdfs:domain>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="created_by"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#patient_of">
    <rdfs:subPropertyOf rdf:resource="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/process.owl#participant_in"/>
    <owl:equivalentProperty>
      <owl:ObjectProperty rdf:ID="theme_of"/>
    </owl:equivalentProperty>
    <owl:inverseOf rdf:resource="#patient"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#recipient">
    <rdfs:subPropertyOf rdf:resource="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/process.owl#participant"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >recipient: in a transaction, the agent on the receiving-end.</rdfs:comment>
    <owl:inverseOf rdf:resource="#recipient_of"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#created_by">
    <rdfs:subPropertyOf rdf:resource="#patient_of"/>
    <owl:inverseOf rdf:resource="#creates"/>
    <rdfs:range rdf:resource="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/action.owl#Creation"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#theme_of">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="#theme"/>
    </owl:inverseOf>
    <rdfs:subPropertyOf rdf:resource="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/process.owl#participant_in"/>
    <owl:equivalentProperty rdf:resource="#patient_of"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#instrument">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >instrument: the thing used to establish the action. Examples: knife, hammer, money</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/process.owl#participant"/>
    <owl:inverseOf rdf:resource="#instrument_in"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#theme">
    <owl:equivalentProperty rdf:resource="#patient"/>
    <rdfs:subPropertyOf rdf:resource="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/process.owl#participant"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >see patient</rdfs:comment>
    <owl:inverseOf rdf:resource="#theme_of"/>
  </owl:ObjectProperty>
  <rdf:Description rdf:about="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/action.owl#Reaction">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/action.owl#actor"/>
        <owl:someValuesFrom>
          <owl:Restriction>
            <owl:someValuesFrom rdf:resource="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/action.owl#Action"/>
            <owl:onProperty rdf:resource="#recipient_of"/>
          </owl:Restriction>
        </owl:someValuesFrom>
      </owl:Restriction>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:about="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/action.owl#Transaction">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            <owl:onProperty rdf:resource="#patient"/>
          </owl:Restriction>
          <owl:Restriction>
            <owl:someValuesFrom rdf:resource="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/action.owl#Agent"/>
            <owl:onProperty rdf:resource="#recipient"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </rdf:Description>
  <rdf:Description rdf:about="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/action.owl#Trade">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:complementOf>
              <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                  <rdf:Description rdf:about="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/action.owl#Agent"/>
                  <rdf:Description rdf:about="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/action.owl#Action"/>
                  <rdf:Description rdf:about="http://darius.lri.jur.uva.nl/svn/LRICore/branches/lricore-2.0/role.owl#Role"/>
                </owl:unionOf>
              </owl:Class>
            </owl:complementOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="#patient"/>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#patient"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >2</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </rdf:Description>
</rdf:RDF>

