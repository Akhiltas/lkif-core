    <rdfs:subClassOf rdf:resource="#Medium"/>
    <rdfs:subClassOf rdf:nodeID="A29"/>
    <owl:equivalentClass rdf:nodeID="A16"/>
  </rdf:Description>
  <rdf:Description rdf:about="#Evidence">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Observation and/or statement, used as a backing for a belief. Evidence is the role of observation which is qualified by some belief.</rdfs:comment>
