    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
  </rdf:Description>
  <rdf:Description rdf:about="#Assertion">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The assertion is subject to a fit between words and world, in Searle's terms. It's propositional content can be true of false. If it is inconsistent with other assertions, beliefs, observations, it may come to be considered false.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Communicated_Attitude"/>
    <owl:equivalentClass rdf:nodeID="A35"/>
  </rdf:Description>
  <rdf:Description rdf:about="#Evaluative_Proposition">
    <owl:equivalentClass rdf:nodeID="A36"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Some thing which is evaluatively qualified, i.e. an evaluation applies to the proposition, the proposition is judged. The proposition is comparable to some other proposition.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="#Proposition"/>
    <owl:equivalentClass rdf:nodeID="A37"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A38">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
