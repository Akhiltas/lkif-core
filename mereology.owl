    <rdfs:subPropertyOf>
      <owl:TransitiveProperty rdf:about="#part_of"/>
    </rdfs:subPropertyOf>
    <owl:inverseOf rdf:resource="#component"/>
  </owl:ObjectProperty>
  <owl:TransitiveProperty rdf:about="#part_of">
    <owl:inverseOf>
      <owl:TransitiveProperty rdf:about="#part"/>
    </owl:inverseOf>
  </owl:TransitiveProperty>
  <owl:TransitiveProperty rdf:about="#part">
    <owl:inverseOf rdf:resource="#part_of"/>
  </owl:TransitiveProperty>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#inverseOf">
    <owl:inverseOf rdf:resource="http://www.w3.org/2002/07/owl#inverseOf"/>
  </rdf:Description>
</rdf:RDF>

