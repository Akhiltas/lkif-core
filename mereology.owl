<?xml version="1.0"?>
<rdf:RDF
    xmlns="http://svn.leibnizcenter.org/svn/Estrella/wp1/ontology/lkif-core/mereology.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
  xml:base="http://svn.leibnizcenter.org/svn/Estrella/wp1/ontology/lkif-core/mereology.owl">
  <owl:Ontology rdf:about="">
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Created with TopBraid Composer</owl:versionInfo>
  </owl:Ontology>
  <owl:Class rdf:ID="Whole">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="Part"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="part"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Composition">
    <owl:equivalentClass>
      <owl:Class>
        <owl:complementOf>
          <owl:Class rdf:ID="Atom"/>
        </owl:complementOf>
      </owl:Class>
    </owl:equivalentClass>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="component"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Add restriction: second-component = first-component.meet</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Part">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#Whole"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="part_of"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Whole"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#part_of"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:ID="Pair">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >2</owl:cardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#component"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#Composition"/>
  </owl:Class>
  <owl:Class rdf:about="#Atom">
    <owl:equivalentClass>
      <owl:Class>
        <owl:complementOf>
          <owl:Restriction>
            <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#part"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:complementOf>
      </owl:Class>
    </owl:equivalentClass>
    <owl:equivalentClass>
      <owl:Class>
        <owl:complementOf rdf:resource="#Composition"/>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="component_of">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="#component"/>
    </owl:inverseOf>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#part_of"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#part">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="#part_of"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#part_of">
    <owl:inverseOf rdf:resource="#part"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="contain"/>
  <owl:ObjectProperty rdf:about="#component">
    <owl:inverseOf rdf:resource="#component_of"/>
    <rdfs:subPropertyOf rdf:resource="#part"/>
  </owl:ObjectProperty>
</rdf:RDF>

