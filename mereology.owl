<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY lkif-top "http://svn.leibnizcenter.org/svn/Estrella/wp1/ontology/lkif-core/lkif-top.owl">
  <!ENTITY mereology "http://svn.leibnizcenter.org/svn/Estrella/wp1/ontology/lkif-core/mereology.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="&mereology;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about="">
    <owl:imports>
      <owl:Ontology rdf:about="&lkif-top;"/>
    </owl:imports>
    <owl:versionInfo rdf:datatype="&xsd;string">LKIF-Core, version 1.0, copyright (C) 2007, the ESTRELLA consortium</owl:versionInfo>
    <owl:versionInfo rdf:datatype="&xsd;string">Partner contributions from the University of Amsterdam (UvA) and the University of Bologna (UNIBO).

Authors:      Rinke Hoekstra (UvA), Alexander Boer (UvA), Kasper van den Berg (UvA)
Contributors: Joost Breuker (UvA)</owl:versionInfo>
    <owl:versionInfo rdf:datatype="&xsd;string">The LKIF-Core ontology, version 1.0, is part of Deliverable 1.4 of the European project for Standardised Transparent Representations in order to Extend Legal Accessibility (ESTRELLA, IST-2004-027665), http://www.estrellaproject.org

The use of this ontology, and the OWL files through which it is distributed, is covered by the GNU Lesser General Public License (LGPL), version 2.1 (http://www.gnu.org/copyleft/lesser.html) in as far as the copyright is not covered by additional restrictions of the ESTRELLA consortium agreement (info@estrellaproject.org). The LGPL license places copyright restrictions on the ontology itself but does not apply these restrictions to software that merely links with concepts defined in the ontology library. Please read the license restrictions before using this work.</owl:versionInfo>
    <owl:versionInfo rdf:datatype="&xsd;string">This module is part of LKIF-Core, version 1.0, part of Deliverable 1.4 of ESTRELLA, IST-2004-027665</owl:versionInfo>
  </owl:Ontology>

<!-- Classes -->
  <owl:Class rdf:about="#Atom">
    <rdfs:comment rdf:datatype="&xsd;string">An atom has no parts</rdfs:comment>
    <owl:disjointWith rdf:resource="#Composition"/>
    <owl:disjointWith>
      <owl:Restriction>
        <owl:disjointWith rdf:resource="#Atom"/>
        <owl:onProperty rdf:resource="#part"/>
        <owl:someValuesFrom rdf:resource="&owl;Thing"/>
      </owl:Restriction>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#part"/>
        <owl:someValuesFrom rdf:resource="&owl;Thing"/>
      </owl:Restriction>
    </owl:disjointWith>
  </owl:Class>

  <owl:Class rdf:about="#Composition">
    <rdfs:comment rdf:datatype="&xsd;string">A composition has multiple parts, the components should meet</rdfs:comment>
    <rdfs:subClassOf rdf:resource="&lkif-top;#Abstract_Concept"/>
    <owl:disjointWith rdf:resource="#Atom"/>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#part"/>
        <owl:someValuesFrom rdf:resource="&owl;Thing"/>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>

  <owl:Class rdf:about="#Pair">
    <rdfs:comment rdf:datatype="&xsd;string">A composition of exactly two parts</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Composition"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:cardinality>
        <owl:onProperty rdf:resource="#part"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Part">
    <rdfs:comment rdf:datatype="&xsd;string">A part is a part_of some whole</rdfs:comment>
    <rdfs:subClassOf rdf:resource="&lkif-top;#Abstract_Concept"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Whole"/>
        <owl:onProperty rdf:resource="#part_of"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#part_of"/>
        <owl:someValuesFrom rdf:resource="#Whole"/>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>

  <owl:Class rdf:about="#Whole">
    <rdfs:comment rdf:datatype="&xsd;string">A whole as several parts</rdfs:comment>
    <rdfs:subClassOf rdf:resource="&lkif-top;#Abstract_Concept"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Part"/>
        <owl:onProperty rdf:resource="#part"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#part"/>
        <owl:someValuesFrom rdf:resource="#Part"/>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>

  <owl:Class rdf:about="&lkif-top;#Abstract_Concept"/>

<!-- Annotation Properties -->
  <owl:AnnotationProperty rdf:about="&rdfs;comment"/>
  <owl:AnnotationProperty rdf:about="&owl;versionInfo"/>

<!-- Object Properties -->
  <owl:ObjectProperty rdf:about="#component">
    <rdfs:comment rdf:datatype="&xsd;string">Specifies that some thing is a (functional) component of some other thing</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#part"/>
    <owl:inverseOf rdf:resource="#component_of"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#component_of">
    <rdfs:comment rdf:datatype="&xsd;string">Specifies that some thing is a (functional) component of some other thing</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#part_of"/>
    <owl:inverseOf rdf:resource="#component"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#composed_of">
    <rdfs:comment rdf:datatype="&xsd;string">Specifies that some thing is composed_of (spatially) within some other thing</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#part"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#contained_in">
    <rdfs:comment rdf:datatype="&xsd;string">Specifies that some thing is contained (spatially) within some other thing</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#part_of"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#direct_part">
    <rdfs:comment rdf:datatype="&xsd;string">The non-transitive part relation.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#part"/>
    <owl:inverseOf rdf:resource="#direct_part_of"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#direct_part_of">
    <rdfs:comment rdf:datatype="&xsd;string">The non-transitive part_of relation</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#part_of"/>
    <owl:inverseOf rdf:resource="#direct_part"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#member">
    <rdf:type rdf:resource="&owl;TransitiveProperty"/>
    <rdfs:comment rdf:datatype="&xsd;string">Specifies membership of a set or group</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#part"/>
    <owl:inverseOf rdf:resource="#member_of"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#member_of">
    <rdf:type rdf:resource="&owl;TransitiveProperty"/>
    <rdfs:comment rdf:datatype="&xsd;string">Specifies membership of a set or group</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#part_of"/>
    <owl:inverseOf rdf:resource="#member"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#part">
    <rdfs:comment rdf:datatype="&xsd;string">Transitive part relation</rdfs:comment>
    <owl:inverseOf rdf:resource="#part_of"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#part_of">
    <rdfs:comment rdf:datatype="&xsd;string">Transitive part_of relation</rdfs:comment>
    <owl:inverseOf rdf:resource="#part"/>
  </owl:ObjectProperty>
</rdf:RDF>
