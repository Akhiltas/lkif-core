<?xml version="1.0"?>
<rdf:RDF
    xmlns:role="http://svn.leibnizcenter.org/svn/Estrella/wp1/ontology/lkif-core/role.owl#"
    xmlns="http://svn.leibnizcenter.org/svn/Estrella/wp1/ontology/lkif-core/process.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:spacetime="http://svn.leibnizcenter.org/svn/Estrella/wp1/ontology/lkif-core/spacetime.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:top="http://svn.leibnizcenter.org/svn/Estrella/wp1/ontology/lkif-core/lkif-top.owl#"
    xmlns:daml="http://www.daml.org/2001/03/daml+oil#"
    xmlns:place="http://svn.leibnizcenter.org/svn/Estrella/wp1/ontology/lkif-core/relative-places.owl#"
  xml:base="http://svn.leibnizcenter.org/svn/Estrella/wp1/ontology/lkif-core/process.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://svn.leibnizcenter.org/svn/Estrella/wp1/ontology/lkif-core/role.owl"/>
